<div class="modal fade" id="lihatRevisiPb2" tabindex="-1" role="dialog" aria-labelledby="lihatRevisiPb2Label" 
aria-hidden="true">
        <!-- Komprehensif Form -->
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
              <form action="/mahasiswa/komprehensif/edit" method="post" enctype="multipart/form-data">
                <div class="modal-header">
                    <h4>Lihat Revisi Pembimbing 2</h4>
                </div>
                <div class="modal-body container">
                  <div class="row">
                    <div class="col">
                      <div class="form-group">
                        <label for="npm">NPM</label>
                        <input class="form-control" type="number" name="npm" id="npm" value="<%= profile.npm %>" disabled>
                      </div>
                      <div class="form-group">
                        <label for="nama">Nama</label>
                        <input class="form-control" type="text" name="nama" id="nama" value="<%= profile.nama %>" disabled>
                      </div>
                      <div class="form-group">
                        <label style="color: #4682B4; font-weight: bold;" for="dosenPembimbing2">Dosen Pembimbing 2:</label>
                        <span style="color: #4682B4; font-weight: bold;"><%= komprehensif.dosenPembimbing2 %></span>
                        </div>
                      <div class="form-group">
                        <label for="revisi2Komprehensif">Revisi</label>
                        <textarea class="form-control" name="revisi2" id="revisi2Komprehensif" style="height: 300px;" disabled><%= komprehensif.revisi2Komprehensif %></textarea>
                      </div>
                      <div class="form-group">
                        <br>
                        <label for="komprehensifRevisiDosen2">Revisi Bimbingan Komprehensif dari Dosen: </label>
                        <% if (komprehensif.komprehensifRevisiDosen1) { %>
                          <a href="/mahasiswa/komprehensif-revisi/download-dosen1/<%= komprehensif.id %>">
                            <%= komprehensif.komprehensifRevisiDosen2.split('/').pop() %>
                          </a>
                        <% } else { %>
                          -
                        <% } %> 
                      </div>                      
                      <div class="form-group">
                        <label for="tanggalRevisiDosen2Komprehensif">Tanggal Dosen Memberikan Revisi</label>
                        <input class="form-control" type="datetime-local" name="tanggalRevisiDosen2" id="tanggalRevisiDosen2Komprehensif" value="<%= komprehensif.tanggalRevisiDosen2Komprehensif ? new Date(komprehensif.tanggalRevisiDosen2Komprehensif).toLocaleString('sv', { timeZone: 'Asia/Jakarta' }).slice(0, 16) : '' %>" disabled>
                    </div>   
                  </div>    
                </div>
              </div>                                            
                <div class="modal-footer">
                  <button type="button" class="btn" style="background-color: #325662; color: #FFF;" data-dismiss="modal">Kembali</button>
                </div>
              </form>
            </div>
          </div>
        </div>
      <!-- /.container-fluid -->
