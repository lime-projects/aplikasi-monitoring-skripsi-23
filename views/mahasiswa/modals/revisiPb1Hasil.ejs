<div class="modal fade" id="lihatRevisiPb1" tabindex="-1" role="dialog" aria-labelledby="lihatRevisiPb1Label" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <form action="/mahasiswa/hasil-skripsi-revisi" method="post" enctype="multipart/form-data">
                <div class="modal-header">
                    <h4>Lihat Revisi Pembimbing 1</h4>
                </div>
                <div class="modal-body container">
                    <div class="row">
                        <div class="col">
                            <div class="form-group">
                                <label for="npm">NPM</label>
                                <input class="form-control" type="number" name="npm" id="npm" value="<%= npm %>" disabled>
                            </div>
                            <div class="form-group">
                                <label for="nama">Nama</label>
                                <input class="form-control" type="text" name="nama" id="nama" value="<%= nama %>" disabled>
                            </div>
                            <% dataHasilSkripsi.map((item, i) => { %>
                            <div class="form-group">
                              <label style="color: #4682B4; font-weight: bold;" for="dosenPembimbing1">Dosen Pembimbing 1:</label>
                              <span style="color: #4682B4; font-weight: bold;"><%= item.DevisionOfLecturer.dosenPembimbing1 %></span>
                              <% }) %> 
                            </div>                          
                            <div class="form-group">
                                <label for="revisi1">Revisi</label>
                                <textarea class="form-control" name="revisi1" id="revisi1" style="height: 300px;" disabled><%= hasilSkripsi.revisi1 %></textarea>
                            </div>
                            <div class="form-group">
                              <br>
                              <label for="hasilSkripsiRevisiDosen1">Revisi Hasil Skripsi dari Dosen: </label>
                              <% if (hasilSkripsi.hasilSkripsiRevisiDosen1) { %>
                                <a href="/mahasiswa/hasil-skripsi-revisiDosen1/download/<%= hasilSkripsi.id %>">
                                  <%= hasilSkripsi.hasilSkripsiRevisiDosen1.split('/').pop() %>
                                </a>
                              <% } else { %>
                                -
                              <% } %> 
                            </div>                      
                            <div class="form-group">
                              <label for="tanggalRevisiDosen1">Tanggal Dosen Memberikan Revisi</label>
                              <input class="form-control" type="datetime-local" name="tanggalRevisiDosen1" id="tanggalRevisiDosen1" value="<%= hasilSkripsi.tanggalRevisiDosen1 ? new Date(hasilSkripsi.tanggalRevisiDosen1).toLocaleString('sv', { timeZone: 'Asia/Jakarta' }).slice(0, 16) : ''%>" disabled>
                          </div>                            
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn" style="background-color: #325662; color: #FFF;" data-dismiss="modal">Kembali</button>
                </div>
            </form>
        </div>
    </div>
  </div>
  