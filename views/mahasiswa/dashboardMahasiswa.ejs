<%- include('../layouts/header') %>
    <div id="wrapper">
        <%- include('sidebarMahasiswa') %>
            <!-- Content Wrapper -->
            <div id="content-wrapper" class="d-flex flex-column">

                <!-- Main Content -->
                <div id="content">

                    <!-- Navbar Mahasiswa -->
                    <%- include('../layouts/navbarTopMahasiswa', { id, username }) %>

                        <!-- Begin Page Content -->
                        <div class="container-fluid">

                            <!-- Page Heading -->
                            <div class="d-sm-flex align-items-center justify-content-between mb-4">
                                <h1 class="h3 mb-0 text-gray-800">
                                    <%= title %>
                                </h1>
                            </div>

                              <div style="color: #325662;" style="font-size: 30px;">
                                <strong>JUDUL :</strong>
                              </div>
                              <div class="text-kesamping" style="color: #325662; display: flex; align-items: center; margin-right: 10px; font-size: 18px;">
                                <% if (judul) { %>
                                  <% let judulFound = false; %>
                                  <% judul.forEach(item => { %>
                                    <% if (item.TitleSubmission) { %>
                                      <% if (item.TitleSubmission.statusPersetujuan === 'Judul 1 Diterima') { %>
                                        <% if (item.TitleSubmission.judul1) { %>
                                          <div class="judul-container">
                                            <%= item.TitleSubmission.judul1 %>
                                          </div>
                                          <% judulFound = true; %>
                                        <% } %>
                                      <% } else if (item.TitleSubmission.statusPersetujuan === 'Judul 2 Diterima') { %>
                                        <% if (item.TitleSubmission2.judul2) { %>
                                          <div class="judul-container">
                                            <%= item.TitleSubmission2.judul2 %>
                                          </div>
                                          <% judulFound = true; %>
                                        <% } %>
                                      <% } %>
                                    <% } %>
                                  <% }); %>
                                  <% if (!judulFound) { %>
                                    <div style="color: orange;">Anda belum mengajukan judul</div>
                                  <% } %>
                                <% } else { %>
                                  Data tidak ditemukan
                                <% } %>
                              </div>
                              <br>
                              
                              
                            <!-- Content Row -->
                            <div class="row">

                                <!-- Earnings (Monthly) Card Example -->
                                <div class="col-xl-3 col-md-6 mb-4">
                                    <div class="card border-left-primary shadow h-100 py-2">
                                        <div class="card-body">
                                            <div class="row no-gutters align-items-center">
                                                <div class="col mr-2">
                                                    <div class="text-xs font-weight-bold text-primary text-uppercase mb-1">
                                                        Jumlah Mahasiswa Skripsi
                                                    </div>
                                                    <div class="h5 mb-0 font-weight-bold text-gray-800"><%= jmlMhs.length %></div>
                                                </div>
                                                <div class="col-auto">
                                                    <i class="fas fa-user fa-2x text-gray-300"></i>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!-- Earnings (Monthly) Card Example -->
                                <div class="col-xl-3 col-md-6 mb-4">
                                    <div class="card border-left-info shadow h-100 py-2">
                                        <div class="card-body">
                                            <div class="row no-gutters align-items-center">
                                                <div class="col mr-2">
                                                    <div class="text-xs font-weight-bold text-info text-uppercase mb-1">
                                                        Dosen Pembimbing 1
                                                    </div>
                                                    <div class="row no-gutters align-items-center">
                                                        <tbody>
                                                          <% if (pembimbing1) { %>
                                                            <tr>
                                                              <td><strong><%= pembimbing1.dosenPembimbing1 || '-' %></strong></td>
                                                            </tr>
                                                          <% } else { %>
                                                            <tr>
                                                              <td>Belum mengajukan dosen</td>
                                                            </tr>
                                                          <% } %>        
                                                        </tbody>
                                                      </div>
                                                </div>
                                                <div class="col-auto">
                                                    <i class="fas fa-users fa-2x text-gray-300"></i>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-xl-3 col-md-6 mb-4">
                                    <div class="card border-left-warning shadow h-100 py-2">
                                        <div class="card-body">
                                            <div class="row no-gutters align-items-center">
                                                <div class="col mr-2">
                                                    <div class="text-xs font-weight-bold text-warning text-uppercase 
                                                    mb-1">
                                                       Dosen Pembimbing 2
                                                    </div>
                                                    <div class="row no-gutters align-items-center">
                                                            <tbody>
                                                                <% if (pembimbing2) { %>
                                                                    <tr>
                                                                        <td><strong><%= pembimbing2.dosenPembimbing2 || '-' %></strong></td>
                                                                    </tr>
                                                                <% } else { %>
                                                                    <tr>
                                                                        <td>Belum mengajukan dosen</td>
                                                                    </tr>
                                                                <% } %>        
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                                <div class="col-auto">
                                                    <i class="fas fa-user-tag fa-2x text-gray-300"></i>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-xl-3 col-md-6 mb-4">
                                    <div class="card shadow h-100 py-2" style="border-left: 5px solid Plum;">
                                        <div class="card-body">
                                            <div class="row no-gutters align-items-center">
                                                <div class="col mr-2">
                                                    <div  style="color: Plum;" class="text-xs font-weight-bold 
                                                    text-uppercase mb-1">
                                                       Dosen Pembahas
                                                    </div>
                                                    <div class="row no-gutters align-items-center">
                                                            <tbody>
                                                                <% if (pembahas) { %>
                                                                    <tr>
                                                                        <td><strong><%= pembahas.dosenPembahas || '-' %></strong></td>
                                                                    </tr>
                                                                <% } else { %>
                                                                    <tr>
                                                                        <td>Belum mengajukan dosen</td>
                                                                    </tr>
                                                                <% } %>        
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                                <div class="col-auto">
                                                    <i class="fas fa-user-clock fa-2x text-gray-300"></i>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                            </div>

                        </div>
                        <!-- /.container-fluid -->

                </div>
                <!-- End of Main Content -->
                <%- include('../layouts/footer') %>