<%- include('../layouts/header') %>
<div id="wrapper">
    <%- include('./sidebarAdmin') %>
    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

        <!-- Main Content -->
        <div id="content">

            <!-- Navbar Admin -->
            <%- include('../layouts/navbarTopAdmin') %>

            <!-- Begin Page Content -->
            <div class="container-fluid">

                <!-- Page Heading -->
                <div class="d-sm-flex align-items-center justify-content-between mb-4">
                    <h1 class="h3 mb-0 text-gray-800"><%= title %></h1>
                    <button type="button" data-toggle="modal" data-target="#addDaftarDosen" class="d-none d-sm-inline-block btn btn-sm shadow-sm" style="background-color: #325662; color: #FFF;">
                        <i class="fas fa-plus fa-sm text-white-50"></i> Add Daftar Dosen
                    </button>

                    <%- include('./modals/addDosenToDaftar2') %>
                </div>

                <!-- Content Row -->
                <!-- Alert -->
                <% if (addSuccess2.length !== 0) { %>
                    <div class="alert alert-success alert-dismissible fade show" role="alert">
                        <strong><%= addSuccess2 %></strong>
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                <% } %>
                <% if (addSuccess3.length !== 0) { %>
                    <div class="alert alert-success alert-dismissible fade show" role="alert">
                        <strong><%= addSuccess3 %></strong>
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                <% } %>
                <% if (addError2.length !== 0) { %>
                    <div class="alert alert-danger alert-dismissible fade show" role="alert">
                        <strong><%= addError2 %></strong>
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                <% } %>
                <% if (deleteSuccess.length !== 0) { %>
                    <div class="alert alert-success alert-dismissible fade show" role="alert">
                        <strong><%= deleteSuccess %></strong>
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                <% } %>
                <% if (validateError.length !== 0) { %>
                    <div class="alert alert-danger alert-dismissible fade show" role="alert">
                        <strong><%= validateError %></strong>
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                <% } %>

                <!-- DataTales Example -->
                <div class="card shadow mb-4">
                    <div class="card-header py-3">
                        <h6 class="m-0 font-weight-bold" style="color: #325662;">Master Data Dosen</h6>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <div class="dt-buttons" style="display: flex; gap: 10px;">
                            <form action="/admin/copy-daftarDosen2" method="POST">
                                <button type="submit" name="export" value="excel" class="btn" style="background-color: #325662; color: #FFF;">
                                    <i></i> Copy
                                </button>
                            </form>
                            <form action="/admin/csv-daftarDosen2" method="POST">
                                <button type="submit" name="export" value="excel" class="btn" style="background-color: #325662; color: #FFF;">
                                    <i></i> CSV
                                </button>
                            </form>
                            <form action="/admin/excel-daftarDosen2" method="POST">
                                <button type="submit" name="export" value="excel" class="btn" style="background-color: #325662; color: #FFF;">
                                    <i></i> Excel
                                </button>
                            </form>
                            <form action="/admin/pdf-daftarDosen2" method="POST">
                                <button type="submit" name="export" value="pdf" class="btn" 
                                style="background-color: #325662; color: #FFF;">
                                    <i></i> PDF
                                </button>
                            </form>
                            <!-- <form action="/admin/print" method="POST"> -->
                                <!-- <button type="submit" name="export" value="pdf" class="btn"  -->
                                <!-- style="background-color: #325662; color: #FFF;"> -->
                                    <!-- <i></i> Print -->
                                <!-- </button> -->
                            <!-- </form> -->
                        </div>                            
                        <br>
                            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                <thead>
                                    <tr>
                                        <th>No</th>
                                        <th>Dosen Pembimbing 2</th>
                                        <th style="width: 10%;">Aksi</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% daftarPb2.map((item, i) => { %>
                                        <tr>
                                            <td><%= i+1 %></td>
                                            <td><%= item.dosenPembimbing2 %></td>
                                            <td style="display: grid; place-items: center;">
                                                <button type="button" class="btn btn-sm btn-danger" data-toggle="modal" data-target="#deleteDaftarDosen<%= item.id %>">
                                                    <i class="fas fa-trash"></i>
                                                </button>
                                                <%- include('../admin/modals/deleteDaftarDosen2', {
                                                    id: item.id,
                                                    nama: item.dosenPembimbing2,
                                                    nip: item.nip
                                                }) %>
                                            </td>
                                        </tr>
                                    <% }) %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

            </div>
            <!-- /.container-fluid -->

        </div>
        <!-- End of Main Content -->
<%- include('../layouts/footer') %>