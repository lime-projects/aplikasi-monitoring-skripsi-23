<div class="modal fade" id="addListAkademik" tabindex="-1" role="dialog" aria-labelledby="addListAkademikLabel" aria-hidden="true">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <form action="/admin/list-tahun-akademik/tambah" method="post">
                <div class="modal-header">
                    <h4>Tambah List Akademik</h4>
                </div>
                <div class="modal-body container">
                    <div class="form-group">
                        <label for="tahunAkademik">Tahun Akademik</label>
                        <!-- <input type="number" class="form-control" name="tahunAkademik" id="tahunAkademik" placeholder="Masukkan Tahun Akademik"> -->
                        <select class="form-control" name="tahunAkademik" id="tahunAkademik" required>
                            <option value="<%= new Date().getFullYear()-6 %>/<%= new Date().getFullYear()-5 %>"><%= 
                            new Date().getFullYear()-6 %>/<%= new Date().getFullYear()-5 %></option>
                            <option value="<%= new Date().getFullYear()-5 %>/<%= new Date().getFullYear()-4 %>"><%= 
                            new Date().getFullYear()-5 %>/<%= new Date().getFullYear()-4 %></option>
                            <option value="<%= new Date().getFullYear()-4 %>/<%= new Date().getFullYear()-4 %>"><%= 
                            new Date().getFullYear()-4 %>/<%= new Date().getFullYear()-3 %></option>
                            <option value="<%= new Date().getFullYear()-3 %>/<%= new Date().getFullYear()-2 %>"><%= new Date().getFullYear()-3 %>/<%= new Date().getFullYear()-2 %></option>
                            <option value="<%= new Date().getFullYear()-2 %>/<%= new Date().getFullYear()-1 %>"><%= new Date().getFullYear()-2 %>/<%= new Date().getFullYear()-1 %></option>
                            <option value="<%= new Date().getFullYear()-1 %>/<%= new Date().getFullYear() %>"><%= new Date().getFullYear()-1 %>/<%= new Date().getFullYear() %></option>
                            <option value="<%= new Date().getFullYear()-0 %>/<%= new Date().getFullYear()+1 %>"><%= 
                            new Date().getFullYear()-0 %>/<%= new Date().getFullYear()+1 %></option>
                            <option value="<%= new Date().getFullYear()+1 %>/<%= new Date().getFullYear()+2 %>"><%=
                            new Date().getFullYear()+1 %>/<%= new Date().getFullYear()+2 %></option>
                            <option value="<%= new Date().getFullYear()+2 %>/<%= new Date().getFullYear()+3 %>"><%=
                            new Date().getFullYear()+2 %>/<%= new Date().getFullYear()+3 %></option>
                            <option value="<%= new Date().getFullYear()+3 %>/<%= new Date().getFullYear()+4 %>"><%=
                            new Date().getFullYear()+3 %>/<%= new Date().getFullYear()+4 %></option>
                            <option value="<%= new Date().getFullYear()+4 %>/<%= new Date().getFullYear()+5 %>"><%=
                            new Date().getFullYear()+4 %>/<%= new Date().getFullYear()+5 %></option>
                            <option value="<%= new Date().getFullYear()+5 %>/<%= new Date().getFullYear()+6 %>"><%=
                            new Date().getFullYear()+5 %>/<%= new Date().getFullYear()+6 %></option>
                            <option value="<%= new Date().getFullYear()+6 %>/<%= new Date().getFullYear()+7 %>"><%=
                            new Date().getFullYear()+6 %>/<%= new Date().getFullYear()+7 %></option>
                            <option value="<%= new Date().getFullYear()+7 %>/<%= new Date().getFullYear()+8 %>"><%=
                            new Date().getFullYear()+7 %>/<%= new Date().getFullYear()+8 %></option><option value="<%= new Date().getFullYear()+8 %>/<%= new Date().getFullYear()+9 %>"><%=
                            new Date().getFullYear()+8 %>/<%= new Date().getFullYear()+9 %></option>
                            <option value="<%= new Date().getFullYear()+9 %>/<%= new Date().getFullYear()+10 %>"><%=
                            new Date().getFullYear()+9 %>/<%= new Date().getFullYear()+10 %></option>
                            <option value="<%= new Date().getFullYear()+10 %>/<%= new Date().getFullYear()+11 %>"><%=
                            new Date().getFullYear()+10 %>/<%= new Date().getFullYear()+11 %></option>
                            <option value="<%= new Date().getFullYear()+11 %>/<%= new Date().getFullYear()+12 %>"><%=
                            new Date().getFullYear()+11 %>/<%= new Date().getFullYear()+12 %></option>
                            <option value="<%= new Date().getFullYear()+12 %>/<%= new Date().getFullYear()+13
                                %>"><%=
                            new Date().getFullYear()+12 %>/<%= new Date().getFullYear()+13 %></option>
                            <option value="<%= new Date().getFullYear()+13 %>/<%= new Date().getFullYear()+14 
                                %>"><%=
                            new Date().getFullYear()+13 %>/<%= new Date().getFullYear()+14 %></option>
                            <option value="<%= new Date().getFullYear()+14 %>/<%= new Date().getFullYear()+15
                                %>"><%=
                            new Date().getFullYear()+14 %>/<%= new Date().getFullYear()+15 %></option>
                            <option value="<%= new Date().getFullYear()+15 %>/<%= new Date().getFullYear()+16
                                %>"><%=
                            new Date().getFullYear()+15 %>/<%= new Date().getFullYear()+16 %></option>
                            <option value="<%= new Date().getFullYear()+16 %>/<%= new Date().getFullYear()+17
                                %>"><%=
                            new Date().getFullYear()+16 %>/<%= new Date().getFullYear()+17 %></option>
                        </select> 
                    </div>
                    <div class="form-group">
                        <label for="semester">Semester</label>
                        <select class="form-control" name="semester" id="semester" required>
                            <option value="" selected disabled>== Pilih Satu ==</option>
                            <option value="Gasal">Gasal</option>
                            <option value="Genap">Genap</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="angkatan" class="text-muted">Angkatan</label>
                        <select class="form-control text-muted" name="angkatan" id="angkatan" required>
                            <option value="" selected disabled>== Pilih Satu ==</option>
                            <option value="2017">2017</option>
                            <option value="2018">2018</option>
                            <option value="2019">2019</option>
                            <option value="2020">2020</option>
                            <option value="2021">2021</option>
                            <option value="2022">2022</option>
                            <option value="2023">2023</option>
                            <option value="2024">2024</option>
                            <option value="2025">2025</option>
                            <option value="2026">2026</option>
                            <option value="2027">2027</option>
                            <option value="2028">2028</option>
                            <option value="2029">2029</option>
                            <option value="2030">2030</option>
                            <option value="2031">2031</option>
                            <option value="2032">2032</option>
                            <option value="2032">2032</option>
                            <option value="2033">3033</option>
                            <option value="2034">2034</option>
                            <option value="2035">2035</option>
                            <option value="2036">2036</option>
                            <option value="2037">2037</option>
                            <option value="2038">2038</option>
                            <option value="2039">2039</option>
                            <option value="2040">2040</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="tanggalMulai">Tanggal Mulai</label>
                        <input class="form-control" type="date" name="tanggalMulai" id="tanggalMulai" required>
                    </div>
                    <div class="form-group">
                        <label for="tanggalAkhir">Tanggal Akhir</label>
                        <input class="form-control" type="date" name="tanggalAkhir" id="tanggalAkhir" required>
                    </div>
                    <p>Status</p>
                    <div class="form-check">
                        <input class="form-check-input" type="radio" name="status" id="aktif" value="<%= true %>" disabled>
                        <label for="status">Aktif</label>
                    </div>
                    <div class="form-check">
                        <input class="form-check-input" type="radio" name="status" id="mati" value="<%= false %>" required>
                        <label for="status">Mati</label>
                    </div>
                    <button type="submit" class="btn" style="background-color: #325662; color: #FFF;">Tambah</button>
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Batal</button>
                </div>
            </form>
        </div>
    </div>
</div>